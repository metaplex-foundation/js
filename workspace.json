{
  "version": 2,
  "projects": {
    "packages/js": {
      "root": "packages/js",
      "projectType": "library",
      "sourceRoot": "packages/js/src",
      "targets": {
        "prebuild": {
          "executor": "nx:run-commands",
          "options": {
            "command": "yarn depcheck --ignore-patterns=test,dist --ignores=buffer",
            "cwd": "packages/js"
          }
        },
        "build": {
          "executor": "nx:run-commands",
          "options": {
            "commands": ["yarn workspace @metaplex-foundation/js build"],
            "parallel": false
          },
          "dependsOn": [
            {
              "target": "prebuild",
              "projects": "self"
            },
            {
              "target": "build",
              "projects": "dependencies"
            }
          ]
        },
        "clean": {
          "executor": "nx:run-commands",
          "options": {
            "commands": ["yarn workspace @metaplex-foundation/js clean"],
            "parallel": false
          }
        },
        "publish": {
          "executor": "nx:run-commands",
          "options": {
            "command": "node ./scripts/publish.mjs packages/js {args.ver} {args.tag}",
            "forwardAllArgs": true
          },
          "dependsOn": [
            {
              "projects": "self",
              "target": "build"
            }
          ]
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/js/**/*.ts"],
            "fix": true
          }
        },
        "test": {
          "executor": "nx:run-commands",
          "options": {
            "dir": "./dist/packages/js",
            "commands": ["yarn workspace @metaplex-foundation/js test"],
            "parallel": false
          },
          "dependsOn": [
            {
              "target": "build",
              "projects": "self"
            }
          ]
        }
      },
      "tags": []
    },
    "packages/js-plugin-aws": {
